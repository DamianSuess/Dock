<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:DockReactiveUIDiSample"
             xmlns:documents="using:DockReactiveUIDiSample.Views.Documents"
             xmlns:tools="using:DockReactiveUIDiSample.Views.Tools"
             xmlns:tools1="clr-namespace:DockReactiveUIDiSample.ViewModels.Tools"
             xmlns:documents1="clr-namespace:DockReactiveUIDiSample.ViewModels.Documents"
             x:Class="DockReactiveUIDiSample.App">
  <Application.DataTemplates>
    <local:ViewLocator />
    <DataTemplate DataType="documents1:DocumentViewModel">
      <documents:DocumentView/>
    </DataTemplate>
    <DataTemplate DataType="tools1:ToolViewModel">
      <tools:ToolView/>
    </DataTemplate>
  </Application.DataTemplates>
  <Application.Styles>
    <FluentTheme />
    <DockFluentTheme />
  </Application.Styles>
</Application>
